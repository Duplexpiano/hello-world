#include <stdio.h>
void hanio( int n, char a, char b, char c);

int main(void)
{
    int n;
    scanf("%d", &n);
    hanio(n, 'a', 'b', 'c');
    return 0;
}
void hanio( int n, char a, char b, char c)
{
    if(n == 1)  printf("%c-->%c\n", a, b);
    else {
        hanio(n - 1, a, c, b);
        printf("%c-->%c\n", a, b);
        hanio(n-1, c, b, a);
    }
}

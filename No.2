#include <stdio.h>

double a3, a2, a1, a0;

double calculate(double x) {
    return a3 * x*x*x + a2 * x*x + a1 * x + a0;
}

int main(void) {
    double a, b, result;

    scanf("%lf %lf %lf %lf", &a3, &a2, &a1, &a0);
    scanf("%lf %lf", &a, &b);
    while (b - a >= 0.01) {
        if (calculate((a + b) / 2) == 0) {
            result = (a + b) / 2;
            break;
        }
        if (calculate(a) == 0) {
            result = a;
            break;
        }
        if (calculate(b) == 0) {
            result = b;
            break;
        }
        if (calculate(a)*calculate((a + b) / 2) >= 0) {
            a = (a + b) / 2;
        }
        else {
            b = (a + b) / 2;
        }
    }
    if (b - a < 0.01) {
        result = (a + b) / 2;
    }
    printf("%.2f\n", result);

    return 0;
}

#include <stdio.h>
#define MAXS 20

void f( char *p );
void ReadString( char *s ); /* 由裁判实现，略去不表 */

int main()
{
    char s[MAXS];

    ReadString(s);
    f(s);
    printf("%s\n", s);

    return 0;
}
void ReadString( char *s ) {
    int i = 0;
    while((s[i] = getchar()) !+ '\n')
        i++;
    s[i] = '\0';
}
void f( char *p )
{
    int i = 0, q = 0, h, temp;
    while(p[i] != '\0')
        i++;
        h = i-1;
    while(q <= h) {
        temp = p[q];
        p[q] = p[h];
        p[h] = temp;
        q++;
        h--;
    }
}

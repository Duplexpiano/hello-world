#include "stdio.h"
#define N 20

int my_strcat(char str1[],int x,char str2[]);

void readline(char a[])
{
  char ch;
  int i=0;
  while((ch=getchar())!='\n' && i<N-1)
   a[i++]=ch;
  a[i]='\0';
  return ;   
}

int main()
 {char a[N],b[N];
 readline(a);
 readline(b);
 if(!my_strcat(a,N,b))
    puts("error!");
  puts(a);
  return 0;
}
int my_strcat(char str1[],int x,char str2[])
{
 	int i = 0, j = 0, m, n = 0;
 	while(str1[i] != '\0')	i++;
 	while(str2[j] != '\0')	j++;
 	j--;
 	if((x-i-1) >= j)
 	{
 		for(m = i; m <= i+j; m++)
 		{
 			str1[m] = str2[n];
 			n++;
		}
		str1[i+j+1] = '\0';
		return 1;
	} else
	{
		for(m = i; m < x-1; m++)
		{
			str1[m] = str2[n];
			n++;
		}
		str1[x - 1] = '\0';
		return 0;
	}
}
